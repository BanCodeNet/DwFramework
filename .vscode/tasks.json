{
    "version": "2.0.0",
    "inputs": [
        {
            "type": "pickString",
            "id": "ExampleProjectName",
            "options": [
                "CoreExample",
                "QuartzExample",
                "RabbitMQExample",
                "SqlSugarExample",
                "TensorFlowExample",
                "WebExample"
            ],
            "description": "请选择需要调试的项目"
        },
        {
            "type": "pickString",
            "id": "TemplateProjectName",
            "options": [
                "SimpleConsole",
            ],
            "description": "请选择需要调试的项目"
        }
    ],
    "tasks": [
        {
            "label": "build example",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-o",
                "${workspaceFolder}/build/${input:ExampleProjectName}",
                "${workspaceFolder}/example/${input:ExampleProjectName}/${input:ExampleProjectName}.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build template",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-o",
                "${workspaceFolder}/build/${input:TemplateProjectName}",
                "${workspaceFolder}/template/${input:TemplateProjectName}/${input:TemplateProjectName}.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build buildtools",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "-o",
                "${workspaceFolder}/build/BuildTools",
                "${workspaceFolder}/utils/BuildTools/BuildTools.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}