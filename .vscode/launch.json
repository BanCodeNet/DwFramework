{
    "version": "0.2.0",
    "inputs": [
        {
            "type": "pickString",
            "id": "ExampleProjectName",
            "options": [
                "CoreExample",
                "QuartzExample",
                "RabbitMQExample",
                "SqlSugarExample",
                "TensorFlowExample",
                "WebExample"
            ],
            "description": "请选择需要调试的项目"
        },
        {
            "type": "pickString",
            "id": "TemplateProjectName",
            "options": [
                "SimpleConsole",
            ],
            "description": "请选择需要调试的项目"
        }
    ],
    "configurations": [
        {
            "name": "Debug Example",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "preLaunchTask": "build example",
            "args": [
                "${input:ExampleProjectName}.dll",
            ],
            "cwd": "${workspaceFolder}/build/${input:ExampleProjectName}",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Debug Template",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "preLaunchTask": "build template",
            "args": [
                "${input:TemplateProjectName}.dll",
            ],
            "cwd": "${workspaceFolder}/build/${input:TemplateProjectName}",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Build Nuget Package",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "preLaunchTask": "build buildtools",
            "args": [
                "BuildTools.dll"
            ],
            "cwd": "${workspaceFolder}/build/BuildTools",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}