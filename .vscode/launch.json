{
    "version": "0.2.0",
    "inputs": [
        {
            "type": "command",
            "id": "exampleProjectName",
            "command": "extension.commandvariable.pickStringRemember",
            "args": {
                "key": "projectName",
                "options": [
                    "SimpleDependencyInjection",
                    "UseNLog"
                ],
                "description": "请选择需要调试的项目"
            }
        },
        {
            "type": "command",
            "id": "templateProjectName",
            "command": "extension.commandvariable.pickStringRemember",
            "args": {
                "key": "projectName",
                "options": [
                    "SimpleConsole"
                ],
                "description": "请选择需要调试的项目"
            }
        },
        {
            "type": "command",
            "id": "utilProjectName",
            "command": "extension.commandvariable.pickStringRemember",
            "args": {
                "key": "projectName",
                "options": [
                    "NugetBuilder"
                ],
                "description": "请选择需要调试的项目"
            }
        }
    ],
    "configurations": [
        {
            "name": "Debug Example",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Example",
            "cwd": "${workspaceFolder}/build/${input:exampleProjectName}",
            "program": "dotnet",
            "args": [
                "${workspaceFolder}/build/${input:exampleProjectName}/${input:exampleProjectName}.dll",
            ],
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Debug Template",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Template",
            "cwd": "${workspaceFolder}/build/${input:templateProjectName}",
            "program": "dotnet",
            "args": [
                "${workspaceFolder}/build/${input:templateProjectName}/${input:templateProjectName}.dll",
            ],
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": "Run Util",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Util",
            "cwd": "${workspaceFolder}",
            "program": "dotnet",
            "args": [
                "${workspaceFolder}/build/${input:utilProjectName}/${input:utilProjectName}.dll",
            ],
            "console": "integratedTerminal",
            "stopAtEntry": false
        }
    ]
}