<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AssemblyName>DwFramework.Rpc</AssemblyName>
        <RootNamespace>DwFramework.Rpc</RootNamespace>
        <Version>1.2.1.6</Version>
        <Authors>DwGoing</Authors>
        <Copyright>DwGoing</Copyright>
        <Description>DwFramework Rpcåº“</Description>
        <PackageLicenseFile>LICENSE</PackageLicenseFile>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <ReleaseVersion>1.0</ReleaseVersion>
        <PackageProjectUrl>https://github.com/Dwgoing/DwFramework/tree/master/Lib.Rpc</PackageProjectUrl>
    </PropertyGroup>
    <ItemGroup>
        <None Remove="Plugins\Cluster\Protobufs\Cluster.proto" />
        <None Remove="Protobufs\Common.proto" />
    </ItemGroup>
    <ItemGroup>
        <None Include="..\..\LICENSE" Link="LICENSE">
            <Pack>True</Pack>
            <PackagePath>
            </PackagePath>
        </None>
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Google.Protobuf" Version="3.13.0" />
        <PackageReference Include="Grpc.Tools" Version="2.31.0">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Grpc.Core" Version="2.31.0" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\Lib.Core\Lib.Core.csproj" />
    </ItemGroup>
    <ItemGroup>
        <Folder Include="Plugins\Cluster\Protobufs\" />
        <Folder Include="Protobufs\" />
    </ItemGroup>
    <ItemGroup>
        <Protobuf Include="Protobufs\Common.proto" GrpcServices="" />
        <Protobuf Include="Plugins\Cluster\Protobufs\Cluster.proto" GrpcServices="Server Client" />
    </ItemGroup>
</Project>