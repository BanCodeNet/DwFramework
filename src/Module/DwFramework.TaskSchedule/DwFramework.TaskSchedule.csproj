<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <ReleaseVersion>5</ReleaseVersion>
        <BuildVersion>0.1.12-20210331.8032</BuildVersion>
        <Version>$(ReleaseVersion).$(BuildVersion)</Version>
        <Authors>DwGoing</Authors>
        <Description>DwFramework 任务调度库</Description>
        <PackageProjectUrl>https://github.com/Dwgoing/DwFramework</PackageProjectUrl>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\DwFramework.Core\DwFramework.Core.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Quartz" Version="3.3.2" />
    </ItemGroup>
    <ItemGroup>
        <None Include="README.md">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
        <None Include="..\..\..\LICENSE" Link="LICENSE">
            <IncludeInPackage>true</IncludeInPackage>
        </None>
    </ItemGroup>
</Project>