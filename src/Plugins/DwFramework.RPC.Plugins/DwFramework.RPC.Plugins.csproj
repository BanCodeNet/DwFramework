  <ItemGroup>
    <ProjectReference Include="..\..\Module\DwFramework.RPC\DwFramework.RPC.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="README.md">
      <IncludeInPackage>true</IncludeInPackage>
    </None>
    <None Include="..\..\..\LICENSE" Link="LICENSE">
      <IncludeInPackage>true</IncludeInPackage>
    </None>
  </ItemGroup>
</Project>